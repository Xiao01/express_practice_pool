
<%- include("./header.ejs") %>
  


    <div class="row">
      <div class="col-lg-12">
        <% if(data.length <=0 ){%>
          <div class="page-header">
              <h1 id="tables"><a href="/users/regist">恭喜您成为本系统第一个用户首位会员，点击注册成功，即可获得万元红包</a></h1>
            </div>
       <% }else{ %>
        <div class="page-header">
          <h1 id="tables"><%= title %></h1>
        </div>
      
        <div class="bs-component">
          <table class="table table-hover">
            <thead>
              <tr class="table-active">
                <th scope="col">ID</th>
                <th scope="col">用户名</th>
                <th scope="col">手机号</th>
                <th scope="col">注册时间</th>
              </tr>
            </thead>
            <tbody>
                <% data.forEach(function(item,index) { %>
				<% if (index%2 ==0) {%>
				 <tr class="table-secondary">
				<%} else{  %>	
				<tr class="table-primary">
				<% } %>	
				<th scope="row"><%= 1+index %></th>
                <td><%= item.username%></td>
                <td><%= item.phone%></td>
                <td><%= item.last_datetime%></td>
              </tr>
              <% }) %>
            </tbody>
          </table>
        </div>
        <%}%>
      </div>
    </div>
  

  <%- include("./footer.ejs") %>
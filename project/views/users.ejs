
<%- include("./header.ejs") %>
  


    <div class="row">
      <div class="col-lg-12">
        <div class="page-header">
          <h1 id="tables"><%= title %></h1>
        </div>

        <div class="bs-component">
          <table class="table table-hover">
            <thead>
              <tr class="table-active">
                <th scope="col">ID</th>
                <th scope="col">用户名</th>
                <th scope="col">密码</th>
                <th scope="col">确认密码</th>
              </tr>
            </thead>
            <tbody>
                <% data.forEach(function(item,index) { %>
				<% if (index%2 ==0) {%>
				 <tr class="table-secondary">
				<%} else{  %>	
				<tr class="table-primary">
				<% } %>	
				<th scope="row"><%= item._id%></th>
                <td><%= item.username%></td>
                <td><%= item.password%></td>
                <td><%= item.password2%></td>
              </tr>
              <% }) %>
            </tbody>
          </table>
        </div>
      </div>
    </div>
  

  <%- include("./footer.ejs") %>